{
  "name": "your-project",
  "version": "1.0.0",
  "description": "A description of your project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --testMatch='<rootDir>/test/unit/**/*.test.js'",
    "test:integration": "jest --testMatch='<rootDir>/test/integration/**/*.integration.test.js'",
    "test:e2e": "NODE_OPTIONS='--max-old-space-size=4096' jest --config=jest.config.js --testMatch='**/test/e2e/**/*.test.js' --runInBand --detectOpenHandles --forceExit",
    "test:all": "NODE_OPTIONS='--max-old-space-size=8192 --trace-warnings' jest --config=jest.config.js --runInBand --verbose --detectOpenHandles --forceExit",
    "test:debug": "NODE_OPTIONS='--max-old-space-size=8192 --trace-warnings' jest --config=jest.config.js --runInBand --verbose --detectOpenHandles --forceExit --testNamePattern='YOUR_TEST_PATTERN'",
    "start": "node app.js"
  },
  "keywords": [
    "keyword1",
    "keyword2"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "mongoose": "^7.4.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "node-abort-controller": "^3.1.1",
    "puppeteer": "^23.10.1"
  }
}
